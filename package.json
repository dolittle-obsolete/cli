{
  "name": "@dolittle/cli",
  "version": "3.1.0",
  "description": "CLI tool for Dolittle",
  "homepage": "https://dolittle.io",
  "author": "Dolittle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dolittle-tools/cli.git"
  },
  "main": "bin/dolittle.js",
  "module": "bin/dolittle.js",
  "files": [
    "bin"
  ],
  "preferGlobal": true,
  "bin": {
    "dolittle": "bin/dolittle.js",
    "dolittle-cluster": "bin/dolittle-cluster.js",
    "dolittle-cluster-info": "bin/dolittle-cluster-info.js",
    "dolittle-cluster-add": "bin/dolittle-cluster-add.js",
    "dolittle-create": "bin/dolittle-create.js",
    "dolittle-create-application": "bin/dolittle-create-application.js",
    "dolittle-create-boundedcontext": "bin/dolittle-create-boundedcontext.js",
    "dolittle-deploy": "bin/dolittle-deploy.js",
    "dolittle-add": "bin/dolittle-add.js",
    "dolittle-add-command": "bin/dolittle-add-command.js",
    "dolittle-add-event": "bin/dolittle-add-event.js",
    "dolittle-add-readmodel": "bin/dolittle-add-readmodel.js",
    "dolittle-add-aggregateroot": "bin/dolittle-add-aggregateroot.js",
    "dolittle-add-query": "bin/dolittle-add-query.js",
    "dolittle-add-queryfor": "bin/dolittle-add-queryfor.js",
    "dolittle-add-eventprocessor": "bin/dolittle-add-eventprocessor.js",
    "dolittle-add-commandhandler": "bin/dolittle-add-commandhandler.js",
    "dolittle-add-concept": "bin/dolittle-add-concept.js",
    "dolittle-add-intconcept": "bin/dolittle-add-intconcept.js",
    "dolittle-add-stringconcept": "bin/dolittle-add-stringconcept.js",
    "dolittle-add-guidconcept": "bin/dolittle-add-guidconcept.js",
    "dolittle-run": "bin/dolittle-run.js",
    "dolittle-veracity": "bin/dolittle-veracity.js",
    "dolittle-veracity-create": "bin/dolittle-veracity-create.js",
    "dolittle-veracity-create-boundedcontext": "bin/dolittle-veracity-create-boundedcontext.js",
    "dolittle-add-feature": "bin/dolittle-add-feature.js"
  },
  "scripts": {
    "build": "rm -rf bin && npx babel Source --ignore **/for_* -d bin --source-maps inline",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@dolittle/build": "~6.2.1"
  },
  "dependencies": {
    "@dolittle/tooling.common": "^3.1.1",
    "args": "^5.0.0",
    "dockerode": "^2.5.7",
    "inquirer": "^6.2.0"
  }
}
